        {
            "id": "cmd_remove_right_02",
            "type": "command",
            "command": "RemoveCharacter",
            "parameters": ["left"]
        },
        {
            "id": "0101Trial01_Sherry002",
            "type": "debate",
            "speaker": "橘雪莉",
            "portrait": "Sherry/Sherry_ArmL1_ArmR1_Default",
            "voice": "0101Trial01_Sherry002",
            "position": "right",
            "debateConfig": {
                "timeLimit": 300,
                "sentences": [
                    {
                        "id": "s4",
                        "text": "对。\n请大家\n仔细观察尸体……",
                        "isWeakPoint": false
                    }
                ],
                "options": []
            }
        },
        {
            "id": "cmd_remove_left_02",
            "type": "command",
            "command": "RemoveCharacter",
            "parameters": ["right"]
        },
        {
            "id": "0101Trial01_Sherry003",
            "type": "debate",
            "speaker": "橘雪莉",
            "portrait": "Sherry/Sherry_ArmL1_ArmR1_Default",
            "voice": "0101Trial01_Sherry003",
            "position": "right",
            "debateConfig": {
                "timeLimit": 300,
                "sentences": [
                    {
                        "id": "s5",
                        "text": "看尸体的下面，\n还有旁边……\n在这些地方都有<link=\"weak\">用血画下</link>\n的蝴蝶绘画！",
                        "isWeakPoint": true,
                        "correctEvidenceId": "Objection_01_01_01_01"
                    }
                ],
                "options": [
                    {
                        "id": "opt_blood",
                        "text": "画画用的不是血",
                        "isCorrect": false,
                        "nextLineId": "0101Trial01_Ema003",
